{
  "name": "@pkerschbaum/cypress-image-snapshot",
  "version": "4.0.1",
  "description": "Cypress bindings for jest-image-snapshot.",
  "repository": "https://github.com/pkerschbaum/cypress-image-snapshot",
  "author": "Patrick Kerschbaum <patrick.kerschbaum@gmail.com>, Jack Cross <jack@palmer.net>",
  "license": "MIT",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=14.18.0",
    "yarn": "^1.22.5"
  },
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint --max-warnings 0 .",
    "prepublish": "npm run build",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "fs-extra": "^9.1.0",
    "glob": "^7.2.0",
    "pkg-dir": "^5.0.0",
    "term-img": "^6.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.1",
    "@changesets/cli": "^2.18.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest-image-snapshot": "^4.3.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cypress": "^8.7.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.4",
    "jest-image-snapshot": "^4.5.1",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "cypress": "^8.7.0",
    "jest-image-snapshot": "^4.5.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --max-warnings 0"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true
    }
  }
}
